<project xmlns="http://maven.apache.org/POM/4.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>co.edu.uco.notas</groupId>
	<artifactId>UCO.Notas.Transversal</artifactId>
	<version>0.0.1-SNAPSHOT</version>
	<name>UCO.Notas.Transversal</name>
	<description>Componente que posee todos los aspectos que pueden ser utilizados de forma trasnversal por la aplicacion</description>
	<build>
		<sourceDirectory>src</sourceDirectory>
		<plugins>
			<plugin>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>3.8.0</version>
				<configuration>
					<source>1.8</source>
					<target>1.8</target>
				</configuration>
			</plugin>
		</plugins>
	</build>
	<repositories>
		<repository>
			<id>feed-uco</id>
			<url>https://pkgs.dev.azure.com/uco-santiagoospina/UCO-Software_II/_packaging/feed-uco/maven/v1</url>
			<releases>
				<enabled>true</enabled>
			</releases>
			<snapshots>
				<enabled>true</enabled>
			</snapshots>
		</repository>
	</repositories>
	<distributionManagement>
		<repository>
			<id>feed-uco</id>
			<url>https://pkgs.dev.azure.com/uco-santiagoospina/UCO-Software_II/_packaging/feed-uco/maven/v1</url>
			<releases>
				<enabled>true</enabled>
			</releases>
			<snapshots>
				<enabled>true</enabled>
			</snapshots>
		</repository>
	</distributionManagement>
	<dependencies>
		<dependency>
			<groupId>co.edu.uco.transversal</groupId>
			<artifactId>UCO.Transversal</artifactId>
			<version>0.0.1-SNAPSHOT</version>
		</dependency>
	</dependencies>
</project>